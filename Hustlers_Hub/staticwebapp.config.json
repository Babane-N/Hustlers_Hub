{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "/assets/*", "/*.css", "/*.js", "/*.png", "/*.jpg", "/*.jpeg", "/*.gif", "/*.ico" ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "mimeTypes": {
    ".json": "text/json",
    ".webmanifest": "application/manifest+json"
  }
}
